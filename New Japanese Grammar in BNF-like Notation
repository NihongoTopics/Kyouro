//実装していないもの：動名詞、再帰活用、再帰曲用、述語としての関係節
//なんか色々間違ってるかもしれないです。ちょいちょい直していきます。

[Clause] := [Sentence]
          | [Relative Clause]
          | [Adverbial Clause]
          | [Complementary Clause]
          | [Nominal Clause]

[Sentence] := [Adverbial Clause] [Sentence]
            | [Declarative Sentence]
            | [Imparative Sentence]
            | [Interrogative Sentence]

[Relatieve Clause] := [Adverbial Clause] [Relative Clause]
                    | [Relative Verb Phrase]
                    | [Relative Adjective Phrase]
                    | [Relative Noun Phrase]
                    | [Relative Copular Phrase]
                    | [Adnominal Phrase]
                    | [Relatiev Adjectival Noun Phrase]
                    
[Adverbial Clause] := [Adverbial Clause] [Adverbial Clause]
                    | [Conjunction Phrase] 
                    | [Complementizer Phrase]
                    | [Adverbial Verb Phrase] 
                    | [Adverbial Adjective Phrase]
                    | [Adverbial Noun Phrase] 
                    | [Adverbial Copular Phrase] 
                    | [Adverbial Adjectival Noun Phrase] 

[Complementary Clause] := [Advebrial Clause] [Complementary Clause]
                        | [Complementary Verb Phrase]
                        | [Complementary Adjective Phrase]
                        | [Complementary Noun Phrase]
                        | [Complementary Copular Phrase]
                        | [Complementary Adjectival Noun Phrase]

[Nominal Clause] := [Relative Clause] [Nominal Clause]
                  | [Relative Verb Phrase] [Nominizer]
                  | [Relative Adjective Phrase] [Nominizer] 
                  | [Nominal Verb Phrase]
                  | [Nomianl Adjective Phrase]
                  | [Nominal Noun Phrase]
                  | [Nominal Copular Phrase]
                  | [Nomianl Adjectival Noun Phrase]

[Declarative Sentence] := [Declarative Predicate Phrase] [Ending] [Period]
[Imparative Sentence] := [Imparative Predicate Phrase] [Ending] [Period]
[Interrogative Sentence] := [Interrogative Predicate Phrase] [Ending] [Question Mark]

[Period] := . | !
          | Empty
[Question Mark] := . | ? | !?
                 | Empty

[Ending] := Nê | Sâ | Nâ | 'yo | 'monn | Empty

[Declarative Predicate Phrase] := [Declarative Verb Phrase]
                                | [Declarative Adjective Phrase]
                                | [Declarative Copular Phrase]
[Imparative Predicate Phrase] := [Imparative Verb Phrase]
                               | [Imparative Adjective Phrase]
[Interrogative Predicate Phrase] := [Interrogative Verb Phrase]
                                  | [Interrogative Adjective Phrase]
                                  | [Interrogative Copular Phrase]

[Declarative Verb Phrase] := [Relative Verb Phrase]
                           | [Ga-predicate Arguments] [Declarative Ga-verb]
                           | [Ga-wo-predicaete Arguments] [Declarative Ga-wo-verb]
                           | [Ga-ni-predicate Arguments] [Declarative Ga-ni-verb]
                           | [Ga-wo-predicate Arguments] [Declarative Ga-wo-ni-verb]
[Declarative Adjective Phrase] := [Ga-predicate Arguments] [Declarative Adjective]
[Declarative Copular Phrase] := [Ga-predicate Arguments] [Nominal Clause] [Declarative Copula] // 「あれは本です」
                              | [Ga-predicate Arguments] [Complementary Clause] [Declarative Copula] // 「彼女は綺麗です」
[Imparative Verb Phrase] := [Ga-predicate Arguments] [Imparative Ga-verb]
                          | [Ga-wo-predicaete Arguments] [Imparative Ga-wo-verb]
                          | [Ga-ni-predicate Arguments] [Imparative Ga-ni-verb]
                          | [Ga-wo-predicate Arguments] [Imparative Ga-wo-ni-verb]
[Imparative Adjective Phrase] := [Ga-predicate Arguments] [Imparative Adjective]
[Interrogative Verb Phrase] := [Declarative Verb Phrase]
                             | [Imparative Verb Phrase]
                             | [Ga-predicate Arguments] [Interrogative Ga-verb]
                             | [Ga-wo-predicaete Arguments] [Interrogative Ga-wo-verb]
                             | [Ga-ni-predicate Arguments] [Interrogative Ga-ni-verb]
                             | [Ga-wo-predicate Arguments] [Interrogative Ga-wo-ni-verb]
[Interrogative Adjective Phrase] := [Declarative Adjective Phrase]
                                  | [Ga-predicate Arguments] [Interrogative Adjective]
[Interrogative Copular Phrase] := [Ga-predicate Arguments] [Nominal Clause] [Interrogative Copula] // 「あれは東京タワーですか？」
                                | [Ga-predicate Arguments] [Complementary Clause] [Interrogative Copula] // 「お母さんはお元気ですか？」
[Relative Verb Phrase] := [Ga-predicate Arguments] [Relative Ga-verb]
                        | [Ga-wo-predicate Arguments] [Relative Ga-wo-verb]
                        | [Ga-ni-predicate Arguments] [Relative Ga-ni-verb]
                        | [Ga-wo-predicate Arguments] [Relative Ga-wo-ni-verb]
[Relative Adjective Phrase] := [Ga-predicate Arguments] [Relative Adjective]
[Relative Noun Phrase] := [Ga-predicate Arguments] [Relative Noun]
[Relative Copular Phrase] := [Ga-predicate Arguments] [Nominal Clause] [Relative Copula] // 「釣りが趣味の」
[Adnominal Phrase] := [Ga-predicate Arguments] [Adnominal]
[Relatiev Adjectival Noun Phrase] := [Ga-predicate Arguments] [Relative Adjectivan Noun]
[Conjunction Phrase] := [Declarative Predicate Phrase] [Conjunction]
[Complementizer Phrase] := [Quote] [Quotizer]
[Adverbial Verb Phrase] := [Ga-predicate Arguments] [Adverbial Ga-verb] 
                         | [Ga-wo-predicate Arguments] [Adverbial Ga-wo-verb] 
                         | [Ga-ni-predicate Arguments] [Adverbial Ga-ni-verb] 
                         | [Ga-wo-predicate Argumetns] [Adverbial Ga-wo-ni-verb]
[Adverbial Adjective Phrase] := [Ga-predicate Arguments] [Adverbial Adjective] 
[Adverbial Noun Phrase] := [Ga-predicate Arguments] [Adverbial Noun]
[Adverbial Copular Phrase] := [Ga-predicate Arguments] [Nominal Clause] [Adverbial Copula]
                            | [Ga-predicate Arguments] [Complementary Clause] [Adverbial Copula] // 「桜が綺麗なんで」
[Adverbial Adjectival Noun Phrase] := [Ga-predicate Arguments] [Adverbial Adjectival Noun] 
[Complementary Verb Phrase] := [Ga-predicate Arguments] [Complementary Ga-verb]
                             | [Ga-wo-predicate Arguments] [Complementary Ga-wo-verb]
                             | [Ga-ni-predicate Arguments] [Complementary Ga-ni-verb]
                             | [Ga-wo-predicate Arguments] [Complementary Ga-wo-ni-verb]
[Complementary Adjective Phrase] := [Ga-predicate Arguments] [Complementary Adjective]
[Complementary Noun Phrase] := [Ga-predicate Arguments] [Complementary Noun]
[Complementary Copular Phrase] := [Ga-predicate Arguments] [Nominal Clause] [Complementary Copula] // 「家が金持ちなん」
                                | [Ga-predicate Arguments] [Complementary Clause] [Complementary Copula] //「桜が綺麗なん」
[Complementary Adjectival Noun Phrase] := [Ga-predicate Arguments] [Complementary Adjectival Noun] // 「髪が綺麗」
[Nominizer] := 'ka | 'no
[Nominal Verb Phrase] := [Nomianl Ga-verb]
                       | [Nominal Ga-wo-verb]
                       | [Nominal Ga-ni-verb]
                       | [Nominal Ga-wo-ni-verb]
[Nominal Adjective Phrase] := [Nominal Adjective]
[Nominal Noun Phrase] := [Nominal Noun]
[Nominal Copular Phrase] := [Nominal Clause] [Nominal Copula]
                          | [Complementary Clause] [Nominal Copula]
[Nominal Adjectival Noun Phrase] := [Nominal Adjectival Noun]

[Declarative Ga-verb] := [Ga-verb Plain Base] [Declarative Plain Verb Ending]
                       | [Ga-verb Infinitive Base] [Declarative Infinitive Verb Ending]
                       | [Ga-verb Past Base] [Declarative Past Verb Ending]
